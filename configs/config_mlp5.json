{
"data": {
    "name": "MNIST",
    "image_size": [28, 28]
},
"model": {
    "name": "MLP2layers",
    "n_hidden_layers": 5,
    "dense_units": [128, 256, 512, 1024],
    "dropout_rates": [0.2, 0.2],
    "n_classes": 10,
    "is_saved": true,
    "model_folder": "weights"
},
"training": {
    "optimizer": "rmsprop",
    "loss": "categorical_crossentropy",
    "metrics": ["accuracy"],
    "n_epochs": 10,
    "batch_size": 128,
    "shuffle": true
},
"augmentations": {
},
"report":{
    "export_folder": "jsons",
    "datetimeformat": "%d%m%Y_%Hh%M",
    "datetimeformat_report": "%d.%m.%Y at %H:%M"
}
}
