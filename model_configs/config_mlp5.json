{
"data": {
    "name": "MNIST",
    "image_size": [28, 28]
},
"model": {
    "name": "MLP5layers",
    "n_hidden_layers": 5,
    "dense_units": [512, 512, 512, 512, 512],
    "dropout_rates": [0.0, 0.0, 0.0, 0.2, 0.2],
    "n_classes": 10,
    "is_saved": true,
    "model_folder": "weights"
},
"training": {
    "optimizer": "sgd",
    "optim_config": {},
    "loss": "categorical_crossentropy",
    "metrics": ["accuracy"],
    "n_epochs": 5,
    "batch_size": 16,
    "shuffle": true
},
"augmentations": {
},
"report":{
    "export_folder": "training_logs",
    "datetimeformat": "%d%m%Y_%Hh%M",
    "datetimeformat_report": "%d.%m.%Y at %H:%M"
}
}
