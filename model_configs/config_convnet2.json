{
"data": {
    "name": "MNIST",
    "image_size": [28, 28]
},
"model": {
    "name": "ConvNet2layers",
    "filter_sizes": [32, 64],
    "dense_units": 128,
    "dropout_rates": [0.0, 0.2],
    "n_classes": 10,
    "is_saved": true,
    "model_folder": "weights"
},
"training": {
    "optimizer": "rmsprop",
    "optim_config": {
            "lr": 0.001
        },
    "loss": "categorical_crossentropy",
    "metrics": ["accuracy"],
    "n_epochs": 5,
    "batch_size": 64,
    "shuffle": true
},
"augmentations": {
},
"report":{
    "export_folder": "training_logs",
    "datetimeformat": "%d%m%Y_%Hh%M",
    "datetimeformat_report": "%d.%m.%Y at %H:%M"
}
}
