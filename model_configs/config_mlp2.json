{
"data": {
    "name": "MNIST",
    "image_size": [28, 28]
},
"model": {
    "name": "MLP2layers",
    "n_hidden_layers": 2,
    "dense_units": [512, 512],
    "dropout_rates": [0.2, 0.2],
    "n_classes": 10,
    "is_saved": true,
    "model_folder": "weights"
},
"training": {
    "optimizer": "adam",
    "optim_config": {
            "lr": 0.01
        },
    "loss": "categorical_crossentropy",
    "metrics": ["accuracy"],
    "n_epochs": 5,
    "batch_size": 128,
    "shuffle": true
},
"augmentations": {
},
"report":{
    "export_folder": "training_logs",
    "datetimeformat": "%d%m%Y_%Hh%M",
    "datetimeformat_report": "%d.%m.%Y at %H:%M"
}
}
